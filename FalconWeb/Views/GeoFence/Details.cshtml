@model FalconWeb.Models.GeoArea

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>GeoArea</legend>

    <div class="display-label">LatLongs</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.LatLongs)
    </div>

    <div class="display-label">Name</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>

    <div class="display-label">Description</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.ID }, new { @class = "btn" })
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn" })
</p>
